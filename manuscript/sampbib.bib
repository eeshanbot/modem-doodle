journal = {Polar Science},
keywords = {Auroral oval,Polar ionosphere,Polar patches,Scintillation},
language = {en},
month = {jan},
number = {4},
pages = {574--587},
title = {{GPS scintillation studies in the arctic region during the first winter-phase 2008 Indian Arctic Expedition}},
volume = {4},

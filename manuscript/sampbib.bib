@article{stevens_linking_2016,
	title = {Linking glacially modified waters to catchment-scale subglacial discharge using autonomous underwater vehicle observations},
	volume = {10},
	issn = {1994-0424},
	url = {https://tc.copernicus.org/articles/10/417/2016/},
	doi = {10.5194/tc-10-417-2016},
	abstract = {Measurements of near-ice ( {\textless} 200 m) hydrography and near-terminus subglacial hydrology are lacking, due in large part to the difﬁculty in working at the margin of calving glaciers. Here we pair detailed hydrographic and bathymetric measurements collected with an autonomous underwater vehicle as close as 150 m from the ice–ocean interface of the Saqqarliup sermia–Sarqardleq Fjord system, West Greenland, with modeled and observed subglacial discharge locations and magnitudes. We ﬁnd evidence of two main types of subsurface glacially modiﬁed water (GMW) with distinct properties and locations. The two GMW locations also align with modeled runoff discharged at separate locations along the grounded margin corresponding with two prominent subcatchments beneath Saqqarliup sermia. Thus, near-ice observations and subglacial discharge routing indicate that runoff from this glacier occurs primarily at two discrete locations and gives rise to two distinct glacially modiﬁed waters. Furthermore, we show that the location with the largest subglacial discharge is associated with the lighter, fresher glacially modiﬁed water mass. This is qualitatively consistent with results from an idealized plume model.},
	language = {en},
	number = {1},
	urldate = {2021-07-04},
	journal = {The Cryosphere},
	author = {Stevens, Laura A. and Straneo, Fiamma and Das, Sarah B. and Plueddemann, Albert J. and Kukulya, Amy L. and Morlighem, Mathieu},
	month = feb,
	year = {2016},
	pages = {417--432},
	file = {Stevens et al. - 2016 - Linking glacially modified waters to catchment-sca.pdf:/home/eesh/Zotero/storage/A3GCGGAR/Stevens et al. - 2016 - Linking glacially modified waters to catchment-sca.pdf:application/pdf},
}


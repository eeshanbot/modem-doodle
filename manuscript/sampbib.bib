}

@article{moen_effects_1996,
	title = {Effects of {Moose} {Movement} and {Habitat} {Use} on {GPS} {Collar} {Performance}},
	volume = {60},
	issn = {0022-541X},
	url = {https://www.jstor.org/stable/3802085},
	doi = {10.2307/3802085},
	abstract = {We tested a radiotelemetry collar that uses a Global Positioning System (GPS) unit to calculate animal locations. We placed the collar in a range of cover types and compared locations reported by the collar to differentially-corrected GPS locations. We placed the collar on a free-ranging moose (Alces alces) and determined how selection of cover types, collar movement, and collar orientation affected GPS locations. On or off the moose, the GPS unit collected a location in {\textgreater}90\% of location attempts in areas with no or thin canopy cover, including mature deciduous canopies in winter. Under a mature conifer canopy or a mature deciduous canopy in summer, 60 to 70\% of location attempts were successful. Locations from the GPS unit in the collar were close to the expected precision of non-differentially corrected GPS (within 40 m 50\% of the time and 100 m 95\% of the time). Locations did not have a directional bias. Movement of the moose while a location was being attempted did not affect GPS locations. The moose occasionally laid down so the collar was horizontal. Although this decreased the success of location attempts, {\textless}1\% of location attempts were so affected. GPS radiotelemetry has great promise for expanding our knowledge about hourly, daily, and annual patterns in animal movements and habitat selection.},
	number = {3},
	urldate = {2021-05-17},
	journal = {The Journal of Wildlife Management},
	author = {Moen, Ron and Pastor, John and Cohen, Yosef and Schwartz, Charles C.},
	year = {1996},
	pages = {659--668},
}

@misc{porter_kraken_1992,
	title = {The {KRAKEN} {Normal} {Mode} {Program}},
	url = {https://apps.dtic.mil/sti/citations/ADA252409},
	abstract = {In the late 1970s several normal-mode models existed which were widely used for predicting acoustic transmission-loss in the ocean however, each had its own problems. Typical difficulties included numerical instabilities for certain types of sound-speed profiles and failures to compute a complete set of ocean modes. In short, there was a need for a model that was robust, accurate, and efficient. In order to resolve these problems a new algorithm was developed forming the basis for the KRAKEN normal mode model. Over subsequent years, KRAKEN was greatly extended, with options for modeling ocean environments that are range-independent, range-dependent or fully 3-dimensional. The current version offers a specialist a vast number of options for treating ocean-acoustics problems or more generally acousto-elastic waveguides. On the other hand, it is easy for a less sophisticated user to learn the small subset of tools needed for the common problem of transmission-loss modeling in range-independent ocean environments. This report addresses the need for a more complete users guide to supplement the on-line help files. The first chapters give a fairly technical description of the mathematical and numerical basis of the model. Additional chapters give a simpler description of its use and installation in a manner that is accessible to less scientifically-oriented readers.},
	language = {en},
	urldate = {2021-05-23},
	publisher = {Naval Research Lab Washington D.C.},
	author = {Porter, M. B.},
